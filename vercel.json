{
    "version": 2,
    "build": {
      "env": {
        "DATABASE_URL": "@database_url"
      }
    },
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run build"
        }
      }
    ],
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ]
  }
  